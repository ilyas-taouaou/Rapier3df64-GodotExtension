[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 =     "res://../rust/target/debug/librapier_gdext.so"
linux.release.x86_64 =   "res://../rust/target/release/librapier_gdext.so"
windows.debug.x86_64 =   "res://../rust/target/debug/rapier_gdext.dll"
windows.release.x86_64 = "res://../rust/target/release/rapier_gdext.dll"
macos.debug =            "res://../rust/target/debug/librapier_gdext.dylib"
macos.release =          "res://../rust/target/release/librapier_gdext.dylib"
macos.debug.arm64 =      "res://../rust/target/debug/librapier_gdext.dylib"
macos.release.arm64 =    "res://../rust/target/release/librapier_gdext.dylib"
